config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/ar933x_wmac'
        option htmode 'HT20'
        option txpower '30'
        option country 'US'

config wifi-iface
        option device 'radio0'
        option network 'wifi'
        option mode 'ap'
        option ssid 'OpenWrt'
        option encryption 'psk-mixed'
        option key '<somepassword>'